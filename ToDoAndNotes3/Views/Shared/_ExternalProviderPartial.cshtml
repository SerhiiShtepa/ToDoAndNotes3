<div class="auth-external-provider mt-3">
    <p>Or login with</p>
    <form asp-controller="Account" asp-action="ExternalLogin" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
        <button name="provider" value="Google" class="btn-clear mt-2" type="submit">
            <img src="~/images/google256.png" alt="Google" class="google-image">
        </button>
    </form>
</div>